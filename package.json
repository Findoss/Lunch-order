{
  "name": "lunch-order",
  "version": "1.0.0",
  "description": "",
  "author": "Nikita Stroganov",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --watch",
    "formater": "prettier --config .prettierrc \"./src/**/*.ts\" --write",
    "linter": "eslint . --ext .ts",
    "build": "npx tsc",
    "clear": "npx tsc --clean",
    "start": "cross-env APP=prod pm2 start pm2.config.cjs",
    "prestart": "npm run build && mkdir db",
    "prod": "cross-env APP=prod ts-node src/index.ts",
    "dev": "cross-env APP=dev ts-node src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Findoss/Lunch-order.git"
  },
  "bugs": {
    "url": "https://github.com/Findoss/Lunch-order/issues"
  },
  "homepage": "https://github.com/Findoss/Lunch-order#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run formater && npm run linter"
    }
  },
  "dependencies": {
    "@grammyjs/files": "^1.0.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "effector": "^22.3.0",
    "effector-storage": "^5.0.0",
    "grammy": "^1.11.0",
    "simple-json-db": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "babel-jest": "^28.1.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.2"
  }
}
